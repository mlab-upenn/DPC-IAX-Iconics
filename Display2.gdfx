            <script:ScriptCode Name="ProcessPoint1Code" Type="JScriptNet">
              <x:XData><![CDATA[function ProcessPoint1_DataUpdated(sender : System.Object, e : System.EventArgs)
{
	var ppt = ThisConfiguration.GetDynamicObjectByName("Status");
	var status = ppt.GetValueOfPrimaryDataSource();
	
	if (status == 1){
		ThisWindow.Open("Display3.gdfx", WindowTargetType.Self);
	}
	
}
]]></x:XData>
            </script:ScriptCode>
          </script:ScriptCodeManager.Scripts>
